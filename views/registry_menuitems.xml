<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="registry_list_action" model="ir.actions.act_window">
        <field name="name">Motorcycle Registry list</field>
        <field name="res_model">motorcycle.registry</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Create the first registry!</p>

        </field>
    </record>
    <menuitem name="Kawiil Motorcycle Registry" id="menu_registry" sequence="1" web_icon="kawiil_motors_motorcycle_registration,static/description/icon.png">
        <menuitem id="menu_registry_data" name="Registries" sequence="1">
            <menuitem id="menu_registry_customers" name="Motorcycles" sequence="2" action="registry_list_action">
            </menuitem>
        </menuitem>
    </menuitem>
</odoo>